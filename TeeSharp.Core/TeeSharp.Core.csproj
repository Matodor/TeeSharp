<Project Sdk="Microsoft.NET.Sdk" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <LangVersion>7.2</LangVersion>
    <PackageId>TeeSharp.Core</PackageId>
    <PackageVersion>1.0.0-alpha</PackageVersion>
    <Authors>Matodor</Authors>
    <Description>TeeSharp is implementation of Teeworlds in .NET</Description>
    <Title>TeeSharp.Core</Title>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>First release</PackageReleaseNotes>
    <PackageTags>tee teeworlds teesharp</PackageTags>
    <PackageProjectUrl>https://github.com/Matodor/TeeSharp</PackageProjectUrl>
    <PackageIconUrl>https://github.com/Matodor/TeeSharp/raw/master/res/TeeSharp.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/Matodor/TeeSharp</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Memory" Version="4.5.1" />
  </ItemGroup>

</Project>